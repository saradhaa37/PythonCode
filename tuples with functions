def validate_dob_month(name: str)->str:
    for i in range(12):
        if (month_reference[i] == date_of_month[1]):
            return month_names[i]

date_of_birth=input("Enter DOB")
month_names=('January','February','March','April','May','June','July','August','September','October','November','December')
month_reference=['01','02','03','04','05','06','07','08','09','10','11','12']
date_of_month=date_of_birth.split('-')
print(validate_dob_month(date_of_month))
